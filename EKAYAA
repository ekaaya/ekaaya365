<script type='text/javascript'>
//<![CDATA[
// Pop Up
$(document).ready(function() {
  $('.popup-btn').click(function(e) {
    $('.popup-wrap').fadeIn(500);
    $('.popup-box').removeClass('transform-out').addClass('transform-in');
  $('body').css('overflow','hidden');

    e.preventDefault();
  });
  $('.popup-close').click(function(e) {
    $('.popup-wrap').fadeOut(500);
    $('.popup-box').removeClass('transform-in').addClass('transform-out');
  $('body').css('overflow','auto');

    e.preventDefault();
  });
  $(document).mouseup(function(e){
    var container = $(".popup-box");

    // If the target of the click isn't the container
    if(!container.is(e.target) && container.has(e.target).length === 0){
        $('.popup-wrap').fadeOut(500);
        container.removeClass('transform-in').addClass('transform-out');
    $('body').css('overflow','auto');
    }
  });

  // Batas

  $('.popup-btn2').click(function(e) {
    $('.popup-wrap2').fadeIn(500);
    $('.popup-box2').removeClass('transform-out').addClass('transform-in');
  $('body').css('overflow','hidden');

    e.preventDefault();
  });
  $('.popup-close2').click(function(e) {
    $('.popup-wrap2').fadeOut(500);
    $('.popup-box2').removeClass('transform-in').addClass('transform-out');
  $('body').css('overflow','auto');

    e.preventDefault();
  });
  $(document).mouseup(function(e){
    var container = $(".popup-box2");

    // If the target of the click isn't the container
    if(!container.is(e.target) && container.has(e.target).length === 0){
        $('.popup-wrap2').fadeOut(500);
        container.removeClass('transform-in').addClass('transform-out');
    $('body').css('overflow','auto');
    }
  });
});
//]]>
</script>

